	<queryString language="SQL">
		<![CDATA[SELECT
     bodegas.`bodega_id` AS bodegas_bodega_id,
     bodegas.`nombre` AS bodegas_nombre,
     sucursales.`nombre` AS sucursales_nombre,
     bodegas.`estado` AS bodegas_estado,
     usuarios.`user` AS usuarios_user
